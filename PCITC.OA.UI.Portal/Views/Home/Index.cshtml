@model IEnumerable<PCITC.OA.Model.UserInfo>
@using PCITC.OA.Model

<p>@Html.ActionLink("Create New", "Create")</p>

<table>
    <tr>
        <th>编号</th>
        <th>姓名</th>
        <th>密码</th>
        <th>性别</th>
        <th>电话</th>
        <th></th>
    </tr>
    @foreach (UserInfo item in Model.ToList())
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Passwd</td>
            <td>@item.Gender</td>
            <td>@item.Phone</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id})
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }
</table>